{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ROBOAMP/Axton": "@dev",
        "doctrine/dbal": "v2.4.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "ixudra/curl": "6.*",
        "laravel/framework": "^6",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "nunomaduro/laravel-console-menu": "^2.2",
        "spatie/laravel-newsletter": "^4.2",
        "sunra/php-simple-html-dom-parser": "1.5.2",
        "symfony/psr-http-message-bridge": "^1.1",
        "webpatser/laravel-uuid": "^3.0",
        "zendframework/zend-diactoros": "^2.0"
    },
    "repositories": [
        {   "type":"git","url":"git@github.com:taytus/robo_Directory.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_MyArray.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_files.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_myclass.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_url.git"},
        {   "type":"git","url":"git@github.com:taytus/strings.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_validator.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_db.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_server.git"},
        {   "type":"git","url":"git@github.com:taytus/robo_seeder.git"},
        {   "type":"git","url":"git@github.com:taytus/ROBOCORE.git"},
        {   "type":"git","url":"git@github.com:taytus/ROBO_CLI.git"},

        {
            "type": "path",
            "url": "Package/Axton",
            "options": {
                "symlink": true
            }
        }

    ],
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/browser-kit-testing": "^4.0",
        "laravel/dusk": "^5.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "ROBOAMP/Strings":"dev-master",
        "phpunit/phpunit": "^7.0",
        "roboamp/files": "dev-master",
        "roboamp/myclass": "dev-master",
        "roboamp/url": "dev-master",
        "roboamp/validator": "dev-master",
        "roboamp/Server": "dev-master",
        "roboamp/Seeder": "dev-master",
        "roboamp/ROBOCORE": "dev-master",
        "roboamp/MyArray": "dev-master",
        "roboamp/DB": "dev-master",
        "roboamp/CLI": "dev-master"




    },
    "autoload": {
        "files": [
            "Package/Validator/tests/Feature/Validator_test.php"
        ],
        "classmap": [
            "database",
            "Package/Validator/tests",
            "Package/MyArray/tests",
            "Package/Strings/tests",
            "Package/Files/tests",
            "Package/Directory/tests"
        ],
        "psr-4": {
            "App\\": "app/"
        }

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"


        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
